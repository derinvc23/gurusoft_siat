<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="product_normal_form_gurusoft_view" model="ir.ui.view">
            <field name="name">product.product.gurusoft</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="product.product_normal_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='default_code']" position="before">
                    <field name="siat_unidad_id" required="1"/>
                    <field name="siat_producto_id" required="1"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
